{
  "name": "BobaPal",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "start": "expo start --port 8085",
    "start:dev": "expo start --dev-client --port 8085",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "build:simulator": "eas build --profile development-simulator --platform ios --local",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "update:prod": "eas update --branch production"
  },
  "dependencies": {
    "@aws-amplify/backend": "^1.19.0",
    "@aws-amplify/backend-cli": "^1.8.1",
    "@aws-amplify/react-native": "^1.3.1",
    "@aws-amplify/rtn-web-browser": "^1.3.0",
    "@aws-amplify/ui-react-native": "^2.6.4",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@expo/vector-icons": "^15.0.3",
    "@nozbe/watermelondb": "^0.25.5",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.8.4",
    "@react-navigation/elements": "^2.8.1",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "@react-navigation/stack": "^7.6.3",
    "aws-amplify": "^6.15.9",
    "babel-preset-expo": "^54.0.9",
    "expo": "^54.0.23",
    "expo-blur": "~15.0.8",
    "expo-build-properties": "^1.0.9",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.10",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-location": "~19.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "nativewind": "latest",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-maps": "^1.26.20",
    "react-native-reanimated": "~4.1.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/generator": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@react-native-community/cli": "latest",
    "@react-native/metro-config": "^0.82.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.17",
    "eslint": "^9.39.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react-refresh": "^0.18.0",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3"
  },
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "trustedDependencies": [
    "unrs-resolver"
  ]
}
